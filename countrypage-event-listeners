<script>
// Attach click event listeners to elements with 'heart-full' and 'heart-empty' classes
function addClickListeners() {
  // Get all elements with class 'heart-full'
  const heartFullElements = document.querySelectorAll('.heart-full');
  
  // Loop through each element and add click event listener
  heartFullElements.forEach(element => {
    element.addEventListener('click', function() {
      console.log('heart-full-clicked');  // Log message to console when clicked
    });
  });
  
  // Get all elements with class 'heart-empty'
  const heartEmptyElements = document.querySelectorAll('.heart-empty');
  
  // Loop through each element and add click event listener
  heartEmptyElements.forEach(element => {
    element.addEventListener('click', function() {
      console.log('heart-empty-clicked');  // Log message to console when clicked
    });
  });
}
</script>
